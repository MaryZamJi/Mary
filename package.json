{
  "name": "banesco-retail-journey",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:es": "NODE_OPTIONS=--max-old-space-size=4096 ng serve --configuration=es-DO",
    "start:en": "NODE_OPTIONS=--max-old-space-size=4096 ng serve --configuration=en-US",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "create-translations": "ng xi18n --output-path src --i18n-locale en-US && xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/localize": "^11.2.11",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@backbase/backbase-theme": "2.321.0",
    "@backbase/data-ang": "^3.9.5",
    "@backbase/employee-ang": "^3.9.5-sp.20",
    "@backbase/foundation-ang": "^6.13.1",
    "@backbase/retail-ang": "^3.9.5-sp.20",
    "@backbase/ui-ang": "^5.54.6",
    "@backbase/universal-ang": "^6.9.18",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-select/ng-select": "^5.1.0",
    "@ngneat/until-destroy": "^7.3.0",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/d3-scale": "^4.0.0",
    "@types/googlemaps": "^3.43.3",
    "bignumber.js": "^9.0.1",
    "bootstrap": "~4.3.1",
    "core-js": "~2.6.5",
    "jasmine-marbles": "^0.8.4",
    "jquery": "^3.6.4",
    "jwt-decode": "^3.0.0",
    "ngx-mask": "^12.0.0",
    "rxjs": "~6.6.2",
    "save": "^2.9.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1001.0",
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@angular/language-service": "~10.1.0",
    "@backbase/accessgroup-data-http-ang": "^5.1.1",
    "@backbase/payment-schematics": "^1.0.0-beta.2",
    "@bb-cli/bb-ang": "^5.1.4",
    "@bb-cli/schematics": "5.0.1",
    "@microsoft/api-extractor": "7.12.1",
    "@ngtools/webpack": "^12.0.5",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/jquery": "^3.5.16",
    "@types/node": "^11.11.8",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.0.0",
    "ng-packagr": "11.1.4",
    "prettier": "~2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "engines": {
    "node": "^10.13.0 || ^12.16.1",
    "npm": "^6.11.0"
  },
  "backbase": {
    "defaultScope": "banesco"
  },
  "xliffmergeOptions": {
    "srcDir": "apps/banesco-retail-journey/src",
    "genDir": "apps/banesco-retail-journey/src/locale",
    "i18nFile": "messages.xlf",
    "i18nBaseFile": "messages",
    "i18nFormat": "xlf",
    "encoding": "UTF-8",
    "defaultLanguage": "en-US",
    "languages": [
      "es-DO",
      "en-US"
    ],
    "removeUnusedIds": true,
    "supportNgxTranslate": false,
    "ngxTranslateExtractionPattern": "@@|ngx-translate",
    "useSourceAsTarget": true,
    "targetPraefix": "",
    "targetSuffix": "",
    "beautifyOutput": false,
    "allowIdChange": false,
    "autotranslate": false,
    "apikey": "",
    "apikeyfile": "",
    "verbose": true,
    "quiet": false
  }
}
